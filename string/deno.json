    "test": "deno test -A *.test.ts",
    "lint": "deno lint *.json *.ts",
    "check": "deno check *.json *.ts"
  },
  "imports": {
    "@epdoc/datetime": "jsr:@epdoc/datetime@^2.0.14",
    "@epdoc/type": "jsr:@epdoc/type@^1.2.4",
    "@std/assert": "jsr:@std/assert@^1.0.18"
  }
}