  "imports": {
    "@epdoc/duration": "jsr:@epdoc/duration@^0.2.3",
    "@epdoc/fs": "jsr:@epdoc/fs@^1.0.0-alpha.4",
    "@epdoc/string": "jsr:@epdoc/string@^0.1.4",
    "@epdoc/type": "jsr:@epdoc/type@^1.1.17",
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "@std/expect": "jsr:@std/expect@^1.0.17",
    "@std/fmt": "jsr:@std/fmt@^1.0.8",
    "@std/testing": "jsr:@std/testing@^1.0.15"
  },
  "fmt": {
    "singleQuote": true,
    "lineWidth": 120,
    "exclude": ["fs/tests/readonly", "fs/tests/data1", "fs/tests/data2"]
