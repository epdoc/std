{
  "name": "@epdoc/duration",
  "version": "0.3.7",
  "description": "Duration formatting",
  "exports": {
    ".": "./src/mod.ts"
  },
  "tasks": {
    "docs": "deno doc --json src/mod.ts > library-docs.json",
    "prepublish": "deno fmt && deno lint && deno task check && deno task test && deno task docs",
    "test": "deno test --allow-env",
    "check": "deno check src test *.json examples",
    "lint": "deno lint"
  },
  "imports": {
    "@epdoc/type": "jsr:@epdoc/type@^1.2.4",
    "@std/expect": "jsr:@std/expect@^1.0.17",
    "@std/testing": "jsr:@std/testing@^1.0.17"
  }
}